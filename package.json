{
  "name": "nhs111-resources",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "npm-run-all clean:*",
    "clean:empty-dist": "rimraf dist",
    "clean:create-dist": "mkdir dist",
    "prebuild": "npm run clean",
    "build": "npm-run-all build:* nhs-assets",
    "build:eleventy": "eleventy",
    "build:webpack": "webpack --mode=production",
    "predev": "npm run clean",
    "dev": "npm-run-all --parallel dev:* nhs-assets",
    "dev:webpack": "webpack --watch --mode=development",
    "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve --watch",
    "nhs-assets": "cpx 'node_modules/nhsuk-frontend/packages/assets/**/*.*' 'dist/assets'",
    "start": "npm run dev"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "mini-css-extract-plugin": "^1.3.3",
    "nhsuk-frontend": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "webpack": "^5.10.1",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0"
  }
}

{
  "name": "nhs111-resources",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": "10",
    "node": "20"
  },
  "scripts": {
    "clean": "npm-run-all clean:*",
    "clean:empty-dist": "rimraf dist",
    "clean:create-dist": "mkdir dist",
    "prebuild": "node engine-strict.js && npm run clean",
    "build": "npm-run-all build:* nhs-assets",
    "build:eleventy": "eleventy",
    "build:webpack": "webpack --mode=production",
    "predev": "npm run clean",
    "dev": "npm-run-all --parallel dev:* nhs-assets",
    "dev:webpack": "webpack --watch --mode=development",
    "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve --watch",
    "nhs-assets": "cpx 'node_modules/nhsuk-frontend/packages/assets/**/*.*' 'dist/assets'",
    "start": "npm run dev",
    "preclean": "node engine-strict.js",
    "preinstall": "node engine-strict.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "core-js": "^3.21.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "cssnano": "^5.0.17",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "nhsuk-frontend": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.1",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
